#
# Kerberos client realm configuration for <%= @fqdn %>
#
# WARNING:
#   This file is managed by Puppet via the "<%= @module_name %>" module.

[realms]
 <%= @name %> = {
    kdc = <%= @kdc %>
    admin_server = <%= @admin_server %>
 }

[domain_realm]
 .<%= @domain %> = <%= @name %>
 <%= @domain %> = <%= @name %>
